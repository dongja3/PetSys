<class 
    name="com.drategy.pets.domain.RfidChip" 
    table="rfidChips"
>
    <id
        name="id"
        type="java.lang.String"
        column="id"
    >      
        <generator class="assigned" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        length="20"
    >     
    </property>
    <property
        name="code"
        type="java.lang.String"
        column="code"
        length="255"
    >
    </property>
    <property
        name="type"
        type="java.lang.String"
        column="type"
        length="20"
    >
    </property>
    <property
        name="startDate"
        type="java.lang.String"
        column="startDate"
        length="2"
    >
    </property>
    <property
        name="using"
        type="int"
        column="using"
        length="4"
    >
    </property>
    <property
        name="description"
        type="java.lang.String"
        column="description"
        length="200"
    >
    </property>

