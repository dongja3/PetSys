<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>

	<class name="com.drategy.pets.domain.SmsSend" table="smsSend">

		<id name="id" type="java.lang.String" column="id">
			<generator class="assigned" />
		</id>

		<property name="smsNo" type="java.lang.String"
			column="smsNo" length="50">
		</property>

		<property name="smsContext" type="java.lang.String"
			column="smsContext" length="200">
		</property>

		<property name="smsSendState" type="java.lang.String" column="smsSendState" length="20">
		</property>	
       
        <property name="sendTime" type="int" column="sendTime" length="4">
		</property>	
	</class>
</hibernate-mapping>